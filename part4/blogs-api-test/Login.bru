meta {
  name: Login
  type: http
  seq: 5
}

post {
  url: http://localhost:3001/api/login
  body: json
  auth: inherit
}

body:json {
  {
    "username": "{{username}}",
    "password": "{{password}}"
  }
}

vars:pre-request {
  username: admin
  password: adminpass
}

settings {
  encodeUrl: true
  timeout: 0
}
